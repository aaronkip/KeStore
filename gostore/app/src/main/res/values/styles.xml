<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_back</item>
        <item name="colorAccent">@color/parentcolor</item>

    </style>

    <style name="noActionBar" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/parentcolor</item>
    </style>

    <style name="noActionBar_black" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/black</item>
    </style>

    <style name="AlertDialog" parent="Theme.AppCompat.Light.Dialog.Alert">

    </style>
    <style name="Button.Primary.Borderless" parent="@style/Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="Switch" parent="Theme.AppCompat.Light">
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorSwitchThumbNormal">@color/gray</item>
        <item name="android:colorForeground">@color/gray</item>
    </style>
    <style name="TextRegular">
        <item name="android:textSize">20dp</item>
        <item name="android:fontFamily">@font/neosanspro_medium</item>
    </style>

    <style name="MySeekBar" parent="Widget.AppCompat.SeekBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/white</item>
        <item name="android:progressBackgroundTint" tools:ignore="NewApi">@color/white</item>

    </style>

    <style name="MySeekBar2" parent="Widget.AppCompat.SeekBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:progressBackgroundTint" tools:ignore="NewApi">@color/colorPrimary</item>

    </style>

    <style name="RadioButton" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/parentcolor</item>
        <item name="android:textColorSecondary">@color/graybackground</item>
        <item name="colorControlNormal">@color/graybackground</item>

    </style>



    <style name="datepicker_style" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/parentcolor</item>
    </style>





    <style name="Login_phone_A" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/black</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:windowContentTransitions" tools:targetApi="lollipop">true</item>
    </style>

    <style name="countrypicker_style" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/parentcolor</item>
    </style>




    <style name="DialogStyle" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:background">@color/white</item>
        <item name="colorAccent">@color/parentcolor</item>
        <item name="android:textColorPrimary">@color/black</item>
    </style>

</resources>
